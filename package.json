        "pg": "^8.7.3",
        "pino": "^7.0.0-rc.9"
    },
    "devDependencies": {
        "@types/node": "^18.6.4",
        "@types/pg": "^8.6.5",
        "@typescript-eslint/eslint-plugin": "^5.32.0",
        "@typescript-eslint/parser": "^5.32.0",
        "eslint": "^8.21.0",
        "nodemon": "^2.0.19",
        "npm-run-all": "^4.1.5",
        "pg-hstore": "^2.3.4",
